{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RestAPI": {
	  "URI":"http://localhost:3000/",
    "Endpoints": {
      "Credentials": {
        "Methods": [ "POST" ],
        "URI": "api/credentials"
      },
      "CheckIn": {
        "Methods": [ "POST", "GET" ],
        "URI": "api/checkins"
      },
      "CheckOut": {
        "Methods": [ "POST" ],
        "URI": "api/checkins/{studentid}/checkout"
      }
    }
  }
}
